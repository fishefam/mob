{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "module": "commonjs",
    "paths": {
      "@components/*": ["src/components/*"],
      "@libs/*": ["src/libs/*"]
    },
    "skipLibCheck": true,
    "strict": true,
    "target": "es2016"
  }
}
